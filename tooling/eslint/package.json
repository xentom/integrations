    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-turbo": "^2.5.4",
    "typescript-eslint": "^8.34.1"
  },
  "devDependencies": {
    "@acme/integrations-prettier": "*",
    "@acme/integrations-typescript": "*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@acme/integrations-prettier"
