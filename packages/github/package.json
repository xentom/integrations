{
  "name": "@xentom/integrations-github",
  "displayName": "GitHub",
  "description": "Integrate GitHub to trigger workflows from issues, pull requests or commits and automate actions like creating issues or commenting on pull requests.",
  "version": "0.0.3",
  "type": "module",
  "icon": "./images/icon.png",
  "main": "./build/index.js",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules build dist",
    "dev": "xentom dev",
    "pack": "xentom pack",
    "publish": "xentom publish"
  },
  "dependencies": {
    "@octokit/webhooks": "^14.1.3",
    "@xentom/integration-framework": "catalog:",
    "octokit": "^5.0.3",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@xentom/style-guide": "catalog:",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@xentom/style-guide/prettier"
}