{
  "$schema": "https://assets.xentom.com/schemas/package.json",
  "name": "@xentom/google-sheets",
  "version": "0.0.3",
  "displayName": "Google Sheets",
  "description": "Integration for Google Sheets to read, write, and manage spreadsheet data in workflows.",
  "categories": [
    "Experimental",
    "Productivity"
  ],
  "homepage": "https://xentom.com/integrations/xentom/google-sheets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xentom/integrations.git",
    "directory": "packages/google-sheets"
  },
  "bugs": {
    "url": "https://github.com/xentom/integrations/issues"
  },
  "type": "module",
  "icon": "./assets/icon.png",
  "main": "./build/index.js",
  "scripts": {
    "lint": "biome lint --config-path ../../biome.json .",
    "format": "biome format --config-path ../../biome.json --write .",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules build dist",
    "dev": "xentom dev",
    "build": "xentom build",
    "pack": "xentom pack"
  },
  "dependencies": {
    "@xentom/integration-framework": "catalog:",
    "google-auth-library": "10.3.0",
    "googleapis": "catalog:",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@xentom/style-guide": "catalog:",
    "@types/bun": "catalog:",
    "typescript": "catalog:",
    "@biomejs/biome": "catalog:"
  }
}
