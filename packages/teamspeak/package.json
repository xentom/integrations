{
  "$schema": "https://assets.xentom.com/schemas/package.json",
  "name": "@xentom/teamspeak",
  "version": "0.0.2",
  "displayName": "TeamSpeak",
  "description": "Connect to TeamSpeak servers to manage channels, users, and server settings programmatically.",
  "categories": [
    "Messaging"
  ],
  "homepage": "https://xentom.com/integrations/xentom/teamspeak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xentom/integrations.git",
    "directory": "packages/teamspeak"
  },
  "bugs": {
    "url": "https://github.com/xentom/integrations/issues"
  },
  "type": "module",
  "icon": "./images/icon.png",
  "main": "./build/index.js",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules build dist",
    "dev": "xentom dev",
    "build": "xentom build",
    "pack": "xentom pack",
    "publish": "xentom publish"
  },
  "dependencies": {
    "@xentom/integration-framework": "catalog:",
    "valibot": "catalog:",
    "ts3-nodejs-library": "^3.5.1"
  },
  "devDependencies": {
    "@xentom/style-guide": "catalog:",
    "@types/bun": "catalog:",
    "@types/node": "^24.10.1",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@xentom/style-guide/prettier"
}