{
  "name": "@xentom/slack",
  "displayName": "Slack",
  "description": "Automate Slack messaging workflows and manage workspace content from Xentom.",
  "version": "0.0.1",
  "type": "module",
  "icon": "./images/icon.png",
  "main": "./build/index.js",
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules build dist",
    "dev": "xentom dev",
    "build": "xentom build",
    "pack": "xentom pack",
    "publish": "xentom publish"
  },
  "dependencies": {
    "@slack/web-api": "^7.10.0",
    "@xentom/integration-framework": "catalog:",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@xentom/style-guide": "catalog:",
    "@types/bun": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@xentom/style-guide/prettier"
}
