{
  "$schema": "https://assets.xentom.com/schemas/package.json",
  "name": "@xentom/resend",
  "version": "0.0.1",
  "displayName": "Resend",
  "description": "Integrate Resend to trigger workflows from email events and send emails using the Resend API.",
  "categories": [
    "Messaging"
  ],
  "homepage": "https://xentom.com/integrations/xentom/resend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xentom/integrations.git",
    "directory": "packages/resend"
  },
  "bugs": {
    "url": "https://github.com/xentom/integrations/issues"
  },
  "type": "module",
  "icon": "./assets/icon.png",
  "main": "./build/index.js",
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .cache .turbo node_modules build dist",
    "dev": "xentom dev"
  },
  "dependencies": {
    "@xentom/integration-framework": "catalog:",
    "resend": "^6.4.2",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@xentom/style-guide": "catalog:",
    "@types/bun": "catalog:",
    "typescript": "catalog:",
    "@biomejs/biome": "catalog:"
  }
}
